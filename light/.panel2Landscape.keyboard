  foregroundStyle:
  - "toolbarCheckUpdateButtonForegroundStyleSystemImage"
  - "toolbarCheckUpdateButtonForegroundStyleText"
toolbarCheckUpdateButtonForegroundStyleSystemImage:
  buttonStyleType: "systemImage"
