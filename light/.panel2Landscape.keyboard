floatTargetScale:
  x: 0.45
  "y": 0.8
floatingKeyboardButtonBackgroundStyle:
  buttonStyleType: "geometry"
  cornerRadius: 8.5
  highlightColor: "#E6E6E6"
  highlightLowerEdgeColor: "#898A8D"
  insets:
    bottom: 6
    left: 3
    right: 3
    top: 6
  normalColor: "#FFFFFF"
  normalLowerEdgeColor: "#898A8D"
keyboardBackgroundStyle:
  buttonStyleType: "geometry"
  normalColor: "#ffffff03"
keyboardLayout:
- HStack:
    subviews:
    - Cell: "toolbarSkinButton"
    - Cell: "toolbarUploadButton"
    - Cell: "toolbarDeployButton"
    - Cell: "toolbarToggleEmbeddedButton"
- HStack:
    subviews:
    - Cell: "toolbarHamster3Button"
    - Cell: "toolbarFeedbackButton"
    - Cell: "toolbarCheckUpdateButton"
    - Cell: "toolbarFinderButton"
keyboardStyle:
  backgroundStyle: "keyboardBackgroundStyle"
  insets:
    bottom: 10
    left: 12
    right: 12
    top: 10
toolbarCheckUpdateButton:
